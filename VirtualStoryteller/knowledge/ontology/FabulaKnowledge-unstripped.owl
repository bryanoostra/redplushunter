<!-- Datatype Properties -->
  <owl:DatatypeProperty rdf:about="#add">
    <rdfs:comment rdf:datatype="&xsd;string">List of facts to be added upon a world change.</rdfs:comment>
    <rdfs:domain rdf:resource="#WorldChanges"/>
