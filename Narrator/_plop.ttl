    <fabula:effects rdf:resource="../FabulaKnowledge.owl#TestMove1Effects"/>
    <fabula:interEffects rdf:resource="../FabulaKnowledge.owl#TestMove1InterEffects"/>
    <fabula:preconditions>(swc:fromGeographicArea ?path ?currLoc)</fabula:preconditions>
    <fabula:preconditions>(swc:toGeographicArea ?path ?TARGET)</fabula:preconditions>
    <fabula:preconditions>(swc:isLocated ?TARGET ?multipleAnswers)</fabula:preconditions>
  </rdf:Description>
  <rdf:Description rdf:about="../FabulaKnowledge.owl#ind_BeliefElement.plop.14">
