  </rdf:Description>
  <rdf:Description rdf:about="../FabulaKnowledge.owl#TestMove2">
    <rdf:type rdf:resource="../FabulaKnowledge.owl#TestMove"/>
    <fabula:effects rdf:resource="../FabulaKnowledge.owl#TestMove1Effects"/>
    <fabula:interEffects rdf:resource="../FabulaKnowledge.owl#TestMove1InterEffects"/>
    <fabula:preconditions>(swc:toGeographicArea ?path ?currLoc)</fabula:preconditions>
    <fabula:preconditions>(swc:fromGeographicArea ?path ?TARGET)</fabula:preconditions>
  </rdf:Description>
