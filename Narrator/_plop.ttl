    <rdfs:comment>Anything internal to a character, like a belief, or an emotion, feeling, etc.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="../FabulaKnowledge.owl#FabulaElement"/>
  </rdf:Description>
  <rdf:Description rdf:about="../FabulaKnowledge.owl#Extract">
