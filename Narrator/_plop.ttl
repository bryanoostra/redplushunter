    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment>List of facts to be added upon a world change.</rdfs:comment>
    <rdfs:domain rdf:resource="../FabulaKnowledge.owl#WorldChanges"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
  <rdf:Description rdf:about="../FabulaKnowledge.owl#TestMove3">
    <rdf:type rdf:resource="../FabulaKnowledge.owl#TestMove"/>
    <fabula:effects rdf:resource="../FabulaKnowledge.owl#TestMove1Effects"/>
    <fabula:interEffects rdf:resource="../FabulaKnowledge.owl#TestMove1InterEffects"/>
    <fabula:preconditions>(swc:toGeographicArea ?path ?TARGET)</fabula:preconditions>
    <fabula:preconditions>(swc:fromGeographicArea ?path ?currloc)</fabula:preconditions>
    <fabula:preconditions>(not(swc:attachedBy ?AGENS ?AttachingObject))</fabula:preconditions>
  </rdf:Description>
  <rdf:Description rdf:about="../FabulaKnowledge.owl#TestMove4">
