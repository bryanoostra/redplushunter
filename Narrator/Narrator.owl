    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#lexicon_entry"/>
  </rdf:Property>
  <rdf:Property rdf:ID="morph">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#lexicon_entry"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >morphology, thus whether it's singular or plural, and in case of verbs which person it is</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:ID="concept">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Name of the concept as used in the Fabula</rdfs:comment>
    <rdfs:domain rdf:resource="#lexicon_entry"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>
  <rdf:Property rdf:ID="gen">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The gender of the concept</rdfs:comment>
    <rdfs:domain rdf:resource="#lexicon_entry"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>
  <rdf:Property rdf:ID="root">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#lexicon_entry"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >the root of the word, translated to dutch (e.g. "berg", "vertel", "mooi")</rdfs:comment>
  </rdf:Property>
