  <owl:Ontology rdf:about=""/>
  <rdfs:Class rdf:ID="lexicon_entry"/>
  <rdf:Property rdf:ID="svp">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >the svp of the verb (_op_pakken)</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#lexicon_entry"/>
  </rdf:Property>
  <rdf:Property rdf:ID="prep">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#lexicon_entry"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Preposition of the verb (gaan _naar_)</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:ID="det">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The determiner to be used with the dutch translation</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#lexicon_entry"/>
  </rdf:Property>
  <rdf:Property rdf:ID="PoS">
    <rdfs:domain rdf:resource="#lexicon_entry"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Part of Speech; the part of speech of this verbl; Things like noun, pronoun, verb, adjective, etc.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:ID="deplabel">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
