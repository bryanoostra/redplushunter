  <rdf:Description rdf:about="http://www.owl-ontologies.com/FabulaKnowledge.owl#ind_Hunger.plop.2">
    <fabula:psi_causes rdf:resource="http://www.owl-ontologies.com/StoryWorldCore.owl#ind_AttainGoal.plop.8"/>
    <fabula:character rdf:resource="http://www.owl-ontologies.com/StoryWorldCore.owl#ind_Humanoid.plop.1"/>
  </rdf:Description>
  <fabula:TestMove rdf:about="http://www.owl-ontologies.com/FabulaKnowledge.owl#TestMove2">
    <fabula:effects rdf:resource="http://www.owl-ontologies.com/FabulaKnowledge.owl#TestMove1Effects"/>
    <fabula:interEffects rdf:resource="http://www.owl-ontologies.com/FabulaKnowledge.owl#TestMove1InterEffects"/>
    <fabula:preconditions>(swc:fromGeographicArea ?path ?TARGET)</fabula:preconditions>
